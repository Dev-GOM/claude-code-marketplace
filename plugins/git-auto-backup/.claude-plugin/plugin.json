{
  "name": "git-auto-backup",
  "version": "1.0.0",
  "description": "Automatically backup your work with git commits after each Claude session",
  "author": {
    "name": "Dev GOM"
  },
  "category": "hooks",
  "homepage": "https://github.com/Dev-GOM/claude-code-marketplace",
  "keywords": [
    "productivity",
    "git",
    "backup",
    "automation"
  ],
  "hooks": {
    "stop": {
      "command": "node ${CLAUDE_PLUGIN_ROOT}/backup.js",
      "timeout": 10000
    }
  }
}
